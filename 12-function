            function reUse(n1,n2){
                console.log(n1+n2)
                return n1+ n2
            }
            var r=reUse(a,b)
            reUse(c,d)
            reUse(a,d)
            reUse(b,d)
            reUse(a,c)
            console.log("result",r)
            var s=document.getElementById('selectId');
